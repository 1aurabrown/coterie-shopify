@font-face{
  font-family:"SangBleuSans";
  src:
    url('{{ "SangBleuSans-Light-WebXL.eot?#iefix"| asset_url }}');
  src:
    url('{{ "SangBleuSans-Light-WebXL.eot?#iefix" | asset_url }}') format("eot"),
    url('{{ "SangBleuSans-Light-WebXL.woff2" | asset_url }}') format("woff2"),
    url('{{ "SangBleuSans-Light-WebXL.woff" | asset_url }}') format("woff"),
    url('{{ "SangBleuSans-Light-WebXL.ttf" | asset_url }}') format("truetype");
  font-style: normal;
  font-weight: 400;
  text-rendering: optimizeLegibility;
}

